extends base.pug

block scripts
  script(src='/satic/index.js')

block style
    style
      include ../static/style.css
      include ../static/contact.css

block content
    div.container
        h1 Contact Us
        form(action="/contact",method="post" class="myContact")
            input(type="text", class="myInput",name="name",placeholder="Enter Your Name ")
            input(type="text", class="myInput",name="phone",placeholder="Enter Your Phone ")
            input(type="text", class="myInput",name="email",placeholder="Enter Your Email ")
            input(type="text", class="myInput",name="address",placeholder="Enter Your Address")
            input(type="text", class="myInput",name="concern",placeholder="Enter Your Concern")
            button(class="btn")
                |Submit
        

